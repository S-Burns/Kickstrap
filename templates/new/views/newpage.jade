extends layout

block variables
	- var active = 'newpage'

block content

	include _header

	.container
		.row.spacious
			.col-lg-12
				h1 New Page
		.row
			.col-sm-12
				.alert.alert-info
					h4 Example page
					p This page exists to demonstrate the barebones code of a new page in Kickstrap.
		.row.spacious
			.col-md-8.col-md-offset-2#disqus

block foot
	:coffeescript
		k$.ready ->
			jspm.import('ks:disqus')